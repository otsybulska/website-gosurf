                svg
                  use(xlink:href='/assets/icons/sprite.svg#star')
              li
                svg
                  use(xlink:href='/assets/icons/sprite.svg#star')
              li
                svg
                  use(xlink:href='/assets/icons/sprite.svg#star')
              li
                svg
                  use(xlink:href='/assets/icons/sprite.svg#star')
      .block-slider__info.block-info
